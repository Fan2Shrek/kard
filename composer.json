{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "dragonmantank/cron-expression": "^3.4",
        "phpstan/phpdoc-parser": "<2.0",
        "ramsey/uuid-doctrine": "^2.1",
        "symfony/amqp-messenger": "7.1.*",
        "symfony/asset": "7.1.*",
        "symfony/asset-mapper": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/mercure-bundle": "^0.3.9",
        "symfony/messenger": "7.1.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "7.1.*",
        "symfony/property-info": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/scheduler": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/stimulus-bundle": "^2.21",
        "symfony/twig-bundle": "7.1.*",
        "symfony/ux-icons": "^2.21",
        "symfony/ux-react": "^2.21",
        "symfony/ux-turbo": "^2.21",
        "symfony/yaml": "7.1.*",
        "symfonycasts/sass-bundle": "^0.8.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch < /dev/tty"
        ],
        "mutate:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch --mutate --parallel < /dev/tty"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "friendsofphp/php-cs-fixer": "^3.65",
        "pestphp/pest": "^3.5",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "*",
        "spatie/phpunit-watcher": "^1.24",
        "symfony/debug-bundle": "7.1.*",
        "symfony/maker-bundle": "^1.61",
        "symfony/stopwatch": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*",
        "vincentlanglet/twig-cs-fixer": "^3.5"
    }
}
